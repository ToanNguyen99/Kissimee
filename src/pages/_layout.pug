		link(rel="stylesheet", href="https://cdn.linearicons.com/free/1.0.0/icon-font.min.css")
		link(rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/magnific-popup.js/1.1.0/magnific-popup.min.css')

		link(rel="stylesheet", href="./assets/css/main.min.css")
		title Kissimmee-#{title}

	body(class=bodyClass)
		block header
			include ../components/_global/header/header.pug
		main
			block main
			.booking-fixed
				ul
					li.phone-pc
						a(href="tel:+14073505192") 407-350-5192
					li.booking-pc
						a.book(href="https://www.vagaro.com/loversnailsspakissimmee") Booking
					
		block footer
			include ../components/_global/footer/footer.pug
		
